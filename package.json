{
  "name": "lords-gym-auburn",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "preview:cloudflare": "VITE_BASE_PATH=/ npm run build && npx vite preview --base /",
    "deploy:cloudflare": "VITE_BASE_PATH=/ npm run build && npx wrangler pages deploy dist --project-name=lords-gym-auburn",
    "setup:supabase": "node scripts/setup-supabase.js",
    "setup:supabase:ps1": "powershell -ExecutionPolicy Bypass -File scripts/setup-supabase.ps1",
    "setup:cloudflare": "node scripts/setup-github-cloudflare.js"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.90.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.3",
    "vite": "^5.3.4"
  }
}
